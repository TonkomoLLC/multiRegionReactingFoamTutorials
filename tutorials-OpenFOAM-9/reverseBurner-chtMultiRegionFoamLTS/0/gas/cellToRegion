FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/gas";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inletFuel
    {
        type            zeroGradient;
    }
    inletAir
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    wedgeNeg
    {
        type            wedge;
    }
    wedgePos
    {
        type            wedge;
    }
    gas_to_solid
    {
        type            calculated;
        value           uniform 0;
    }
}


