FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/rightSolid";
    object      G;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -3 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    maxX
    {
        type            calculated;
        value           uniform 0;
    }
    minZ
    {
        type            calculated;
        value           uniform 0;
    }
    maxZ
    {
        type            calculated;
        value           uniform 0;
    }
    rightSolid_to_bottomAir
    {
        type            calculated;
        value           uniform 0;
    }
    rightSolid_to_topAir
    {
        type            calculated;
        value           uniform 0;
    }
    rightSolid_to_heater
    {
        type            calculated;
        value           uniform 0;
    }
}


