FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0/leftSolid";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.1 0 0);

boundaryField
{
    minX
    {
        type            calculated;
        value           uniform (0.1 0 0);
    }
    minZ
    {
        type            calculated;
        value           uniform (0.1 0 0);
    }
    maxZ
    {
        type            calculated;
        value           uniform (0.1 0 0);
    }
    leftSolid_to_bottomAir
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    leftSolid_to_heater
    {
        type            calculated;
        value           uniform (0 0 0);
    }
    leftSolid_to_topAir
    {
        type            calculated;
        value           uniform (0 0 0);
    }
}


// ************************************************************************* //
