FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/topAir";
    object      CH4;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 0 0 0 0 ];

internalField   uniform 0;

boundaryField
{
    maxY
    {
        type            inletOutlet;
        value           uniform 0;
        inletValue      uniform 0;
    }
    minX
    {
        type            fixedValue;
        value           uniform 0.25;
    }
    maxX
    {
        type            zeroGradient;
        value           uniform 0;
    }
    minZ
    {
        type            zeroGradient;
        value           uniform 0;
    }
    maxZ
    {
        type            zeroGradient;
        value           uniform 0;
    }
    topAir_to_rightSolid
    {
        type            zeroGradient;
        value           uniform 0;
    }
    topAir_to_heater
    {
        type            zeroGradient;
        value           uniform 0;
    }
    topAir_to_leftSolid
    {
        type            zeroGradient;
        value           uniform 0;
    }
}


