FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/heater";
    object      H2O;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    minY
    {
        type            calculated;
        value           uniform 0;
    }
    minZ
    {
        type            calculated;
        value           uniform 0;
    }
    maxZ
    {
        type            calculated;
        value           uniform 0;
    }
    heater_to_topAir
    {
        type            calculated;
        value           uniform 0;
    }
    heater_to_rightSolid
    {
        type            calculated;
        value           uniform 0;
    }
    heater_to_leftSolid
    {
        type            calculated;
        value           uniform 0;
    }
    heater_to_bottomAir
    {
        type            calculated;
        value           uniform 0;
    }
}


